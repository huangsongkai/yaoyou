<script type="text/javascript">
    try {
        if (window.userInfo) {
            $('#mycourse').show();
            $('#myShopping').show();
        }
    } catch (e) {}
    $('body').delegate('.add-cart-btn', 'click', function(){
        if (!window.userInfo) {
            sso_operation.popLoginWindow();
            return;
        }
        var item = $(this);
        var goodsId = item.attr('data-id');
        if (goodsId) {
            $.ajax({
                url : '/cart/goods/add',
                type : 'POST',
                data : {'goodsId' : goodsId},
                dataType : 'json',
                success : function(data) {
                    if (data.status == 1) {
                        // 购买成功
                        Boxy.ask("添加购物车成功.", ["去购物车", "暂不"], function(val) {}, {title: "提示信息"});
                        setTimeout(function () {
                            $('.answers > input[value="去购物车"]').click(function () {
                                var newWindow = window.open('http://pay.360class.cn/cart', '_blank');
                                newWindow.focus();
                            });
                        }, 50);
                    } else {
                        Boxy.alert(data.message);
                    }
                },
                error : function() {
                    Boxy.alert('您的网络不稳定,请稍后再试.');
                }
            });
        }
    });
</script>